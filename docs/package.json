{
  "name": "handsontable-documentation",
  "private": true,
  "version": "0.0.0",
  "description": "Handsontable documentation",
  "main": "index.js",
  "author": "Handsoncode <hello@handsontable.com>",
  "repository": {
    "url": "https://github.com/handsontable/handsontable",
    "directory": "docs"
  },
  "scripts": {
    "docs:lint": "eslint --ext .js,.vue,.mjs,.ts .vuepress content netlify",
    "docs:lint:fix": "eslint --ext .js,.vue,.mjs,.ts .vuepress content netlify --fix",
    "docs:scripts:link-assets": "node .vuepress/tools/link-assets.mjs",
    "docs:scripts:generate-legacy-docs-versions": "node .vuepress/tools/generate-legacy-docs-versions.mjs",
    "docs:code-examples:generate-js": "bash .vuepress/tools/code-examples-generator.sh",
    "docs:code-examples:generate-all-js": "bash .vuepress/tools/code-examples-generator.sh --generateAll",
    "docs:code-examples:format-all-ts": "bash .vuepress/tools/code-examples-generator.sh --formatAllTsExamples",
    "docs:start": "npm run docs:scripts:link-assets && cross-env NODE_OPTIONS=--openssl-legacy-provider node ./node_modules/.bin/vuepress dev ./",
    "docs:start:no-cache": "npm run docs:scripts:link-assets && cross-env NODE_OPTIONS=--openssl-legacy-provider node ./node_modules/.bin/vuepress dev --no-cache ./",
    "docs:serve": "http-server -c-1 .vuepress/dist/",
    "docs:build": "cross-env NODE_OPTIONS=--openssl-legacy-provider node .vuepress/tools/build.mjs -- --no-cache",
    "docs:docker:build": "npm run docs:docker:build:testing",
    "docs:docker:build:testing": "npm run docs:scripts:link-assets && cross-env BUILD_MODE=testing npm run docs:build && docker build -f docker/Dockerfile -t docs-md ./",
    "docs:docker:build:staging": "npm run docs:scripts:link-assets && cross-env BUILD_MODE=staging npm run docs:build && docker build -f docker/Dockerfile -t docs-md ./",
    "docs:docker:build:production": "cross-env BUILD_MODE=production npm run docs:build && docker build -f docker/Dockerfile -t docs-md:production ./",
    "docs:build:production:netlify": "cross-env DOCS_LATEST_VERSION=$DOCS_LATEST_VERSION BUILD_MODE=production npm run docs:build",
    "docs:api": "node .vuepress/tools/jsdoc-convert/index.mjs",
    "docs:check-links": "node .vuepress/tools/check-links.js",
    "docs:review": "./.vuepress/tools/review-commit.sh",
    "docs:test:example-checker": "node .vuepress/tools/example-checker",
    "docs:visual-test": "npx playwright test",
    "docs:visual-test:update-screenshot": "npx playwright test -u",
    "postinstall": "patch-package"
  },
  "license": "CC BY 4.0",
  "devDependencies": {
    "eslint": "^7.5.0",
    "@babel/eslint-parser": "^7.18.2",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/plugin-proposal-decorators": "^7.13.5",
    "@babel/plugin-proposal-private-methods": "^7.18.6",
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@babel/plugin-syntax-class-properties": "^7.12.13",
    "@babel/plugin-transform-modules-commonjs": "^7.13.8",
    "@babel/plugin-transform-typescript": "^7.24.7",
    "@babel/preset-env": "^7.13.15",
    "@babel/preset-react": "^7.13.13",
    "@babel/preset-typescript": "^7.13.0",
    "@octokit/rest": "^18.6.7",
    "@playwright/test": "1.45.x",
    "@types/node": "^20.11.27",
    "@vuepress/plugin-active-header-links": "^1.9.10",
    "@vuepress/shared-utils": "^1.9.10",
    "broken-link-checker": "^0.7.8",
    "chalk": "^4.1.1",
    "copy-webpack-plugin": "^5.1.2",
    "cross-env": "^7.0.3",
    "dmd": "^6.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "front-matter": "^4.0.2",
    "fs-extra": "^9.1.0",
    "glob": "^7.2.0",
    "http-server": "^14.1.0",
    "inquirer": "^8.1.1",
    "jsdoc-to-markdown": "^7.0.1",
    "netlify-cli": "^17.34.2",
    "prettier": "^2.8.8",
    "puppeteer": "^23.5.3",
    "replace-in-files": "^3.0.0",
    "rimraf": "^3.0.2",
    "semver": "^5.7.1",
    "vue": "^2.7.14",
    "vue-eslint-parser": "^7.6.0",
    "vue-router": "^3.5.1",
    "vue-tabs-component": "^1.5.0",
    "vuepress": "^1.9.10",
    "vuepress-plugin-sitemap": "^2.3.1",
    "vuepress-plugin-tabs": "^0.3.0",
    "xlsx-renderer": "^2.4.0"
  },
  "overrides": {
    "vuepress-plugin-sitemap": {
      "chalk": "^4.1.1"
    }
  },
  "browserslist": [
    "last 1 version",
    "> 1%",
    "not dead"
  ],
  "dependencies": {
    "@docsearch/js": "^3.6.0",
    "@types/overlayscrollbars": "^1.12.5",
    "codesandbox": "^2.2.3",
    "dayjs": "^1.11.10",
    "dotenv": "^16.4.5",
    "patch-package": "^8.0.0"
  }
}
