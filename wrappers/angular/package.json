    "@angular/cli": "^17.0.0",
    "@angular/compiler-cli": "^17.0.0",
    "@types/jasmine": "3.6.9",
    "@types/node": "12.20.7",
    "jasmine": "3.7.0",
    "jasmine-console-reporter": "3.1.0",
    "puppeteer": "14.3.0",
    "ts-node": "8.3.0",
    "typescript": "5.2.2",
    "@angular-eslint/builder": "~17.3.0",
    "@angular-eslint/eslint-plugin": "~17.3.0",
    "@angular-eslint/eslint-plugin-template": "~17.3.0",
